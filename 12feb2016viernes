@Component{(
//generar un selector para q la pag pueda abrirlo
selector:'mi ap'
templete:'<h1>Hola desde angular 2 {{nombre}} mi edsd es{{edad}}</h>'//manera en q se viasuslaizan los atributos en el navegador
)}
export class Componente1{
nombre:string="Reyna";//atributo
edad:number=22;//incializando atributos

}
